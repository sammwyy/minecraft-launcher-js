{
  "name": "minecraft-launcher-js",
  "author": "sammwy",
  "description": "Minecraft launcher wrapper for JavaScipt with modern Typings.",
  "version": "0.0.2",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "husky install && yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "module": "lib/index.esm.js",
  "dependencies": {
    "checksum": "^1.0.0",
    "md5": "^2.3.0",
    "node-fetch": "^2.6.7",
    "typed-emitter": "^2.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/checksum": "^0.1.33",
    "@types/jest": "29.2.4",
    "@types/md5": "^2.3.2",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-plugin-jest": "^27.1.7",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammwyy/minecraft-launcher-js.git"
  },
  "bugs": {
    "url": "https://github.com/sammwyy/minecraft-launcher-js/issues"
  },
  "keywords": [
    "minecraft",
    "minecraft-launcher"
  ]
}
